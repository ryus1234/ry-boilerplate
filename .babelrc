{
  "env": {
    "development": {
      "presets": [
        "es2015",
        "react",
        [
          "env",
          {
            "targets": {
              "Chrome": 57
            }
          }
        ]
      ],
      "sourceMaps": "inline"
    },
    "production": {
      "presets": [
        "es2015",
        "react",
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions",
                "IE 11"
              ]
            }
          }
        ]
      ]
    }
  }
}